{
  "name": "frontend-app-fsd",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:f1eeman/frontend-app-fsd.git",
  "author": "f1eeman <mr.f1eeman@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "24.11.0",
    "npm": "please-use-yarn",
    "yarn": ">= 1.22.22"
  },
  "scripts": {
    "dev": "webpack serve --env port=3001",
    "build:prod": "webpack --env mode=production",
    "build:dev": "webpack --env mode=development",
    "analyze:prod": "webpack --env mode=production --env analyze=enabled",
    "analyze:dev": "webpack --env mode=development --env analyze=enabled",
    "lint:es": "eslint . --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives",
    "lint:es:fix": "yarn run lint:es --fix",
    "lint:es:quiet": "yarn run lint:es --quiet",
    "lint:es:fix:quiet": "yarn run lint:es:quiet --fix",
    "lint:scss": "yarn stylelint \"**/*.scss\"",
    "lint:scss:fix": "yarn run lint:scss --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,scss,json}\"",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "sb": "storybook dev --no-open -p 6006 -c config/storybook ",
    "build:sb": "storybook build -c config/storybook -o ./sb-static",
    "test:ui": "test-storybook --url http://127.0.0.1:6006 --config-dir config/storybook --testTimeout=60000",
    "test:ui:approve": "test-storybook --url http://127.0.0.1:6006 --config-dir config/storybook --testTimeout=60000 --updateSnapshot"
  },
  "devDependencies": {
    "@babel/core": "~7.29.0",
