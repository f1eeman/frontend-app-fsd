    "playwright": "~1.58.2",
    "prettier": "~3.6.2",
    "sass": "~1.97.3",
    "sass-loader": "~16.0.6",
    "storybook": "^10.2.8",
    "style-loader": "~4.0.0",
    "stylelint": "^17.1.1",
